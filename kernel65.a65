	.org $E000

	; make reset vector address not contain any $00 bytes for easier debugging
	.advance $E001

reset:	; reset entry point
	LDA $1001
	ADC #$01
        STA $1001
	inc $d020
        LDA $1002
	ADC #$00
	STA $1002
        LDA $1003
	ADC #$00
	STA $1003
        LDA $1004
	ADC #$00
	STA $1004
	JMP reset

irq:	RTI

nmi:	RTI

	; 6502 interrupt and reset vectors
	.advance $FFFA
	.word nmi
	.word reset
	.word irq

	.outfile "kernel65.bin"
